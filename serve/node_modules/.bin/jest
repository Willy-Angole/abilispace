#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/willy/workplace/Lifetime/Grassroots/shiriki/serve/node_modules/.pnpm/jest@29.7.0_@types+node@20._8b876c6440599e9c37ef4dd39f09f013/node_modules/jest/bin/node_modules:/mnt/c/Users/willy/workplace/Lifetime/Grassroots/shiriki/serve/node_modules/.pnpm/jest@29.7.0_@types+node@20._8b876c6440599e9c37ef4dd39f09f013/node_modules/jest/node_modules:/mnt/c/Users/willy/workplace/Lifetime/Grassroots/shiriki/serve/node_modules/.pnpm/jest@29.7.0_@types+node@20._8b876c6440599e9c37ef4dd39f09f013/node_modules:/mnt/c/Users/willy/workplace/Lifetime/Grassroots/shiriki/serve/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/willy/workplace/Lifetime/Grassroots/shiriki/serve/node_modules/.pnpm/jest@29.7.0_@types+node@20._8b876c6440599e9c37ef4dd39f09f013/node_modules/jest/bin/node_modules:/mnt/c/Users/willy/workplace/Lifetime/Grassroots/shiriki/serve/node_modules/.pnpm/jest@29.7.0_@types+node@20._8b876c6440599e9c37ef4dd39f09f013/node_modules/jest/node_modules:/mnt/c/Users/willy/workplace/Lifetime/Grassroots/shiriki/serve/node_modules/.pnpm/jest@29.7.0_@types+node@20._8b876c6440599e9c37ef4dd39f09f013/node_modules:/mnt/c/Users/willy/workplace/Lifetime/Grassroots/shiriki/serve/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/jest@29.7.0_@types+node@20._8b876c6440599e9c37ef4dd39f09f013/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../.pnpm/jest@29.7.0_@types+node@20._8b876c6440599e9c37ef4dd39f09f013/node_modules/jest/bin/jest.js" "$@"
fi
